@model IEnumerable<EventType>

<h2>All Event Types</h2>

<div style="display: block">
    <a class="btn btn-outline-light" href="/EventType/InsertEventType/">Create a New Event Type</a>
</div>
<p> </p>
<div id="table-pages">
    <table class="table">
        <tr>
            <th>EventType Name</th>
            <th>Update or Delete Event Type</th>
        </tr>

        @foreach (var eventType in @Model)
        {
            <tr>
                <td>@eventType.EventTypeName</td>
                <td><a href="/EventType/UpdateEventType/@eventType.EventTypeID"> @eventType.EventTypeName</a></td>

            </tr>
        }

    </table>
</div>


